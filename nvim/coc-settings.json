{
  "python.jediEnabled": false,
  "python.setInterpreter": "~/.pyenv/versions/nvim3/bin/python",
  "solargraph.commandPath": "/usr/local/lib/ruby/gems/2.6.0/bin/solargraph",
  "yaml.schemas": {
      "https://json.schemastore.org/ansible-role-2.9": "roles/*/tasks/*.yml",
      "https://json.schemastore.org/ansible-playbook": "*playbook*.yml",
      "Kubernetes": "*/*.yaml"
  },
  "languageserver": {
    "golang": {
      "command": "~/go/bin/gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    }
  }
}
